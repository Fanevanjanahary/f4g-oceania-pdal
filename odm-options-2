
/home/useruser/OpenDroneMap/build/bin/odm_georef -bundleFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/opensfm/bundle_r000.out -inputTransformFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/opensfm/geocoords_transformation.txt -inputCoordFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_georeferencing/coords.txt -inputFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_texturing/odm_textured_model.obj -outputFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_texturing/odm_textured_model_geo.obj -inputPointCloudFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/smvs/smvs_dense_point_cloud.ply -outputPointCloudFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_georeferencing/odm_georeferenced_model.ply  -logFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_georeferencing/odm_georeferencing_log.txt -outputTransformFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_georeferencing/odm_georeferencing_transform.txt -georefFileOutputPath /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_georeferencing/odm_georeferencing_model_geo.txt

/home/useruser/OpenDroneMap/SuperBuild/build/pdal/bin/pdal pipeline -i /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_georeferencing/las.json --readers.ply.filename=/mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_georeferencing/odm_georeferenced_model.ply

./SuperBuild/build/pdal/bin/pdal translate -i "/mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_georeferencing/odm_georeferenced_model.laz" -o "/mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_georeferencing/odm_georeferenced_model.filtered.las" decimation outlier range --filters.decimation.step=90 --filters.outlier.method=radius --filters.outlier.radius=2 --filters.outlier.min_k=2 --filters.range.limits='Classification![7:7]'

./SuperBuild/build/pdal/bin/pdal info --boundary --filters.hexbin.edge_length=1 --filters.hexbin.threshold=0 /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_georeferencing/odm_georeferenced_model.filtered.las > /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_georeferencing/odm_georeferenced_model.boundary.json

./SuperBuild/build/pdal/bin/pdal info --summary /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_georeferencing/odm_georeferenced_model.laz > /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_georeferencing/odm_georeferenced_model.summary.json

ogr2ogr -overwrite -a_srs "+proj=utm +zone=37 +datum=WGS84 +units=m +no_defs" /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_georeferencing/odm_georeferenced_model.bounds.shp /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_georeferencing/odm_georeferenced_model.bounds.geojson

/home/useruser/OpenDroneMap/build/bin/odm_georef -bundleFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/opensfm/bundle_r000.out -inputTransformFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/opensfm/geocoords_transformation.txt -inputCoordFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_georeferencing/coords.txt -inputFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_texturing_25d/odm_textured_model.obj -outputFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_texturing_25d/odm_textured_model_geo.obj   -logFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_25dgeoreferencing/odm_georeferencing_log.txt -outputTransformFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_25dgeoreferencing/odm_georeferencing_transform.txt -georefFileOutputPath /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_25dgeoreferencing/odm_georeferencing_model_geo.txt

/home/useruser/OpenDroneMap/build/bin/odm_orthophoto -inputFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_texturing_25d/odm_textured_model_geo.obj -logFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_orthophoto/odm_orthophoto_log.txt -outputFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_orthophoto/odm_orthophoto.png -resolution 15.3861335338  -outputCornerFile /mnt/extravolume/ZMI/ODMProjects/msimbasi_test1/odm_orthophoto/odm_orthophoto_corners.txt
